    function convertCelsiusToFahrenheit(oC)
    {
        return oF

    }
     oC=6
    oF = (oC*9/5) +32
    console.log(convertCelsiusToFahrenheit())


    //ans13
    function bmi(height,weight){
        return BMI
    }
    let height2=156
    let weight2=45
    let BMI = weight2/ (height2 * height2)
    console.log(bmi())
    if (BMI<18.5){
        console.log("Underweight")
    }
    else if(BMI>18.5 && BMI<24.9){
        console.log("ormal weight")
    }
    else if(BMI>25 && BMI<29.9){
        console.log("Overweight")
    }
    else if(BMI>=30 ){
        console.log("Obese")
    }
    

    //ans14
    function checkseason(month)
        {
            if(month=="january" || month=="february" || month=="december")
            {
                console.log("winter")
            }
            else if(month=="march" || month=="april" || month=="may")
            {
                console.log("spring")
            }
            else if(month=="june" || month=="july" || month=="august")
            {
                console.log("summer")
            }
            else if(month=="september" || month=="october" || month=="november")
            {
                console.log("autumn")
            }
        }
        let month="july";
        checkseason(month);


    //ans15
    function findMax(a3,b3,c3){
        return findmax
    }
    let a3=0
    let b3=10
    let c3=5
    if(a3>b3 && a3>c3){
        console.log("a3 is max")
    }
    if(b3>a3 && b3>c3){
        console.log("b3 is max")
    }
    else{
        console.log("c3 is max")
    }
    



    
    


    









